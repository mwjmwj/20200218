    <!-- 特码走势图 -->
    <select id="getSpecialCodeTrendChart" resultType="com.cp.bean.BiyLottery">
    SELECT
    t1.id as id,
    t1.period AS period,
      num AS s_code,
      num as code
       FROM `t_lottery` t1 LEFT JOIN `t_lottery_detail` t2 ON t1.id = t2.tl_id

      AND t2.code_type = 1
      WHERE 1=1 AND SUBSTR(t1.period,1,4) = #{lotteryYear} and t1.end_flag = 1
  </select>

